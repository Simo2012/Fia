
{% spaceless %}
    <!DOCTYPE html>
    <html lang="fr">
        <head>
            
            {% include 'SceauBundle:Site/Common:header.html.twig' %}
        </head>
        <body class="fs">
            <div id="container">
                <div id="header">
                    {% include 'SceauBundle:Site/Common:topblock.html.twig' %}
                </div>
                {% block CONTENT %}{% endblock %}
                    <div id="footer">
                        {% include "SceauBundle:Site/Common:footer.html.twig" %}
                    </div>
           </div>
            
        </body>
        
        {% block JAVASCRIPT %}
            {% javascripts
                '//code.jquery.com/jquery-1.10.2.js'
                '//code.jquery.com/ui/1.11.4/jquery-ui.js'
                'bundles/sceau/js/site/common/commun.js'
                'bundles/sceau/js/site/home/SliderSitePremium.js'
                'bundles/sceau/js/site/home/annuaire.js'
                'bundles/sceau/js/site/home/mdr.js'
                'bundles/sceau/js/site/home/XHRConnection.js'
                'bundles/sceau/js/site/home/popup.js'
                'bundles/sceau/js/site/home/membre.js'
                'http://cdn.jquerytools.org/1.2.6/full/jquery.tools.min.js'
               
                
            %}
                <script src="{{ asset_url }}" type="text/javascript"></script>
                 <script type="text/javascript">
                     $(document).ready(function() {
                        $("#bloc_sitepremiumrandom > ul").tabs({fx:{opacity: "toggle"}}).tabs("rotate", 10, true);
                    });
                    $(function() {
                        $("#newsletter-success-popup").overlay({
                            effect: 'default',
                            top: 'center',
                            mask: {
                                color: '#000000',
                                loadSpeed: 200,
                                opacity: 0.5
                            },
                            close: '#mon-compte-popup-close',
                            closeOnClick: true,
                            load: true,
                            oneInstance: true
                        });
                    });
                    
        
                </script>
               
               
            {% endjavascripts %}  
        {% endblock %}
    </html>
    
{% endspaceless %}