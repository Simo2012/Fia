    {#<div id="lineChart" style="display: inline-block; margin-left: 10px;"></div>#}
<div class="profile-container">
    <!-- begin profile-section -->
    <div class="profile-section">
        <!-- begin profile-left -->
        <div class="profile-left">
            <!-- begin profile-image -->
            <div class="profile-image">
